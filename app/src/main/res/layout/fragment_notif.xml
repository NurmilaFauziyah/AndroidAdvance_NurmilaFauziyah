<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C53938"
    tools:context=".NotifActivity">

    <TextView
        android:id="@+id/tv_rectanglePopUp3"
        android:layout_width="295dp"
        android:layout_height="282dp"
        android:background="@drawable/rectangle_schedule"
        android:backgroundTint="@color/white"
        android:layout_marginTop="280dp"
        android:layout_marginStart="52dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036"
        tools:ignore="MissingConstraints,UselessLeaf" />

    <TextView
        android:id="@+id/tv_rectanglePopUp4"
        android:layout_width="295dp"
        android:layout_height="137dp"
        android:background="@drawable/rectangle_schedule"
        android:backgroundTint="#F5F5F5"
        android:layout_marginTop="425dp"
        android:layout_marginStart="52dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036"
        tools:ignore="MissingConstraints,UselessLeaf" />

    <ImageView
        android:id="@+id/iv_editProfile"
        android:layout_width="200dp"
        android:layout_height="140dp"
        app:srcCompat="@drawable/popup_notifpict2"
        android:layout_marginStart="100dp"
        android:layout_marginTop="280dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036"
        tools:ignore="MissingConstraints,UselessLeaf"/>

    <TextView
        android:id="@+id/tv_textSchedule1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="95dp"
        android:fontFamily="@font/montserratbold"
        android:text="Fix Si Merahâ€™s Body"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_editProfile"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_timeSchedule2"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="95dp"
        android:background="@drawable/time2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_textSchedule1_1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_textSchedule1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="130dp"
        android:fontFamily="@font/montserratbold"
        android:text="Friday, 17 November 2023"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_textSchedule1_1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_textSchedule1_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="130dp"
        android:fontFamily="@font/montserratregular"
        android:text="Today"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_textSchedule1_2"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_yes"
        android:layout_width="97dp"
        android:layout_height="37dp"
        android:backgroundTint="@color/primary"
        android:background="@drawable/costum_button"
        android:text="Done"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="12sp"
        android:fontFamily="@font/montserratbold"
        android:layout_marginTop="10dp"
        android:layout_marginStart="130dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_textSchedule1_3"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_discard"
        android:layout_width="97dp"
        android:layout_height="37dp"
        android:background="@drawable/costum_edittext"
        android:text="Reschedule"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:fontFamily="@font/montserratbold"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="130dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_textSchedule1_3"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>